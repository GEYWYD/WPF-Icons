<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="clr-namespace:WPF_Icons;assembly=WPF-Icons">
    <Geometry x:Key="more_down_outline_rounded">
        M10 38V16h3v19h19v3Zm10-10V6h3v19h19v3Z
    </Geometry>
    <Geometry x:Key="more_down_filled_rounded">
        M10 38V16h3v19h19v3Zm10-10V6h3v19h19v3Z
    </Geometry>
    <Geometry x:Key="more_down_outline_sharp">
        M10 38V16h3v19h19v3Zm10-10V6h3v19h19v3Z
    </Geometry>
    <Geometry x:Key="more_down_filled_sharp">
        M10 38V16h3v19h19v3Zm10-10V6h3v19h19v3Z
    </Geometry>
    <Style x:Key="more_down"
           TargetType="controls:Icon"
           BasedOn="{StaticResource {x:Type controls:Icon}}">
        <Setter Property="RoundedFilledGeometry"
                Value="{StaticResource more_down_filled_rounded}" />
        <Setter Property="RoundedOutlineGeometry"
                Value="{StaticResource more_down_outline_rounded}" />
        <Setter Property="SharpFilledGeometry"
                Value="{StaticResource more_down_filled_sharp}" />
        <Setter Property="SharpOutlineGeometry"
                Value="{StaticResource more_down_outline_sharp}" />
    </Style>

</ResourceDictionary>