<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="clr-namespace:WPF_Icons;assembly=WPF-Icons">
    <Geometry x:Key="hide_outline_rounded">
        M8.1 42 6 39.9l10.7-10.7h-5.9v-3h11v11h-3v-5.9Zm18.15-20.25v-11h3v5.9L39.9 6 42 8.1 31.35 18.75h5.9v3Z
    </Geometry>
    <Geometry x:Key="hide_filled_rounded">
        M8.1 42 6 39.9l10.7-10.7h-5.9v-3h11v11h-3v-5.9Zm18.15-20.25v-11h3v5.9L39.9 6 42 8.1 31.35 18.75h5.9v3Z
    </Geometry>
    <Geometry x:Key="hide_outline_sharp">
        M8.1 42 6 39.9l10.7-10.7h-5.9v-3h11v11h-3v-5.9Zm18.15-20.25v-11h3v5.9L39.9 6 42 8.1 31.35 18.75h5.9v3Z
    </Geometry>
    <Geometry x:Key="hide_filled_sharp">
        M8.1 42 6 39.9l10.7-10.7h-5.9v-3h11v11h-3v-5.9Zm18.15-20.25v-11h3v5.9L39.9 6 42 8.1 31.35 18.75h5.9v3Z
    </Geometry>
    <Style x:Key="hide"
           TargetType="controls:Icon"
           BasedOn="{StaticResource {x:Type controls:Icon}}">
        <Setter Property="RoundedFilledGeometry"
                Value="{StaticResource hide_filled_rounded}" />
        <Setter Property="RoundedOutlineGeometry"
                Value="{StaticResource hide_outline_rounded}" />
        <Setter Property="SharpFilledGeometry"
                Value="{StaticResource hide_filled_sharp}" />
        <Setter Property="SharpOutlineGeometry"
                Value="{StaticResource hide_outline_sharp}" />
    </Style>

</ResourceDictionary>