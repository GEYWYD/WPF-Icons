<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="clr-namespace:WPF_Icons;assembly=WPF-Icons">
    <Geometry x:Key="square_outline_rounded">
        M9 42q-1.25 0-2.125-.875T6 39V9q0-1.25.875-2.125T9 6h30q1.25 0 2.125.875T42 9v30q0 1.25-.875 2.125T39 42Zm0-3h30V9H9Zm0 0V9v30Z
    </Geometry>
    <Geometry x:Key="square_filled_rounded">
        M9 42q-1.25 0-2.125-.875T6 39V9q0-1.25.875-2.125T9 6h30q1.25 0 2.125.875T42 9v30q0 1.25-.875 2.125T39 42Z
    </Geometry>
    <Geometry x:Key="square_outline_sharp">
        M6 42V6h36v36Zm3-3h30V9H9Zm0 0V9v30Z
    </Geometry>
    <Geometry x:Key="square_filled_sharp">
        M6 42V6h36v36Z
    </Geometry>
    <Style x:Key="square"
           TargetType="controls:Icon"
           BasedOn="{StaticResource {x:Type controls:Icon}}">
        <Setter Property="RoundedFilledGeometry"
                Value="{StaticResource square_filled_rounded}" />
        <Setter Property="RoundedOutlineGeometry"
                Value="{StaticResource square_outline_rounded}" />
        <Setter Property="SharpFilledGeometry"
                Value="{StaticResource square_filled_sharp}" />
        <Setter Property="SharpOutlineGeometry"
                Value="{StaticResource square_outline_sharp}" />
    </Style>

</ResourceDictionary>